<div id="stage">
  <canvas id="grid" height="<%= @grid.height %>" , width="<%= @grid.width %>" data-placements="<%= @grid.placements.to_json %>" data-pixel-size="<%= @grid.pixel_size %>"></canvas>
</div>
<button id="reset-zoom" class="btn btn-primary" onclick="stage.style.transform = 'matrix(0.85, 0, 0, 0.85, 525, 15)'">Reset View</button>
